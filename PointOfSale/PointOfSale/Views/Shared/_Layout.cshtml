                    var dropdownOpened = false;

                    // Function to check if there's a message and update the notification counter
                    function checkMessage() {
                        // Replace this with the logic to check if there are any messages available
                        var messagesAvailable = true; // Set this to true if there are messages, false otherwise
                        var notificationCounter = document.getElementById("notificationCounter");
                        var messageBox = document.getElementById("messageBox");

                        if (messagesAvailable) {
                            // If there are messages, show the notification counter
                            notificationCounter.style.display = "block";

                            // Update the notification counter to "1"
                            notificationCounter.textContent = "1";

                            
                        } else {
                            // If there are no messages, hide the notification counter
                            notificationCounter.style.display = "none";

                            
                        }
                    }

                    // Function to hide the message dropdown
                    function hideDropdown() {
                        var messageBox = document.getElementById("messageBox");
                        messageBox.style.display = "none";
                        dropdownOpened = false; // Reset the dropdownOpened flag
                    }

                    // Function to toggle the message dropdown
                    function toggleMessage() {
                        var messageBox = document.getElementById("messageBox");

                        // Toggle the display of the dropdown
                        if (messageBox.style.display === "none") {
                            messageBox.style.display = "block";
                            dropdownOpened = true;
                        } else {
                            messageBox.style.display = "none";
                            dropdownOpened = false;
                        }
                    }

                    // Call checkMessage() to check for updates whenever needed
                    // For example, you can call it in response to user interactions or events
                    checkMessage();
                    
                </script>
