    <style>
        .sa-icon{
            margin-bottom: 35px !important;
        }

        div.sweet-alert > p.lead {
            margin-bottom: 10px !important;
        }


        /* Replace this with your desired styling for the bell icon */
        .bell-icon {
            font-size: 24px;
            cursor: pointer;
            position: relative;
        }
        /* Replace this with your desired styling for the dropdown message */
        .dropdown-message {
            display: none;
            position: absolute;
            bottom: -200px; /* Adjust the value as per your requirement */
            left: -460px; /* Adjust the value as per your requirement */
            width: 500px;
            background-color: #FFFFFF;
            padding: 10px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
        }
        /* Style for the close button */
        .close-button {
            cursor: pointer;
            float: right;
            font-size: 14px;
            color: #888;
           
        }
        /* Remove margin and padding from paragraphs inside the dropdown */
        .dropdown-message p {
            margin: 0;
            padding: 0;
        }
        /* Replace this with your desired styling for the notification counter */
        .notification-counter {
            position: absolute;
            top: 0;
            right: 0;
            background-color: red;
            color: white;
            padding: 4px 8px;
            border-radius: 50%;
            font-size: 9px;
        }
    
    </style>
